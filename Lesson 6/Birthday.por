Algoritmo "Aniversário"

Var

   ano_atual, ano_nascimento, idade: Inteiro;

Inicio

   Escreva("Insira o ano atual: ")
   Leia(ano_atual)
   
   Escreva("Insira o ano em que você nasceu: ")
   Leia(ano_nascimento)
   
   idade <- ano_atual - ano_nascimento
   
   Escreva("Sua idade é: ", idade)

Fimalgoritmo